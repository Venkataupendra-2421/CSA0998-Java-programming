import java.util.Scanner;

public class Main {
  public static void main(String[] args) 
  {
    int i=1,fact=1;
    Scanner input = new Scanner (System.in);
    System.out.print("Enter required number: ");
    int n = input.nextInt();
    while(i<=n){
        fact=fact*i;
        i++;
    }
    
   
     System.out.println("Factorial of  is: "+fact);
  }
}
